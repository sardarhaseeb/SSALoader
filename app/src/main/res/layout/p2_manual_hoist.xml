<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="870dp"
        android:orientation="vertical"
        android:layout_height="match_parent"
        >

        <LinearLayout
            android:layout_width="850dp"
            android:orientation="vertical"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:background="@drawable/output_line_for_indiviudal_tasks">

            <LinearLayout
                android:layout_width="match_parent"
                android:orientation="vertical"
                android:background="#CEBBC9"
                android:layout_height="match_parent"
                android:layout_margin="5dp">




            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Operational Modes"
                android:textSize="30sp"
                android:layout_marginLeft="15dp" />

            <LinearLayout
                android:layout_width="wrap_content"
                android:orientation="horizontal"
                android:background="#CEBBC9"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp">
<LinearLayout
    android:layout_width="380dp"
    android:layout_height="wrap_content"
    android:orientation="horizontal">

    <CheckBox

        android:text="Up  (FAST-F3)"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_marginLeft="50dp"
        android:id="@+id/p2_manual_hoist_operational_upfastf3_checkbox"
        android:textSize="30sp"
        android:textColorLink="?android:attr/colorPressedHighlight" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/grey_p1board"

        android:layout_marginLeft="51dp"
        android:id="@+id/p2_manual_hoist_operational_upf3fast_result_led" />

</LinearLayout>





                <CheckBox
                    android:text="Down(FAST -F4)"
                    android:layout_width="wrap_content"
                    android:textSize="30sp"
                    android:layout_height="wrap_content"

                    android:layout_marginLeft="100dp"
                    android:id="@+id/p2_manual_hoist_operational_downfastf4_checkbox" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:srcCompat="@drawable/grey_p1board"
                    android:layout_marginLeft="55dp"
                    android:id="@+id/p2_manual_hoist_operational_downf4fast_result_led" />


            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:orientation="horizontal"
                android:background="#CEBBC9"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginBottom="10dp">

                <LinearLayout
                    android:layout_width="380dp"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                <CheckBox
                    android:text="Up (SLOW-F3)"
                    android:layout_width="wrap_content"

                    android:layout_height="wrap_content"
                    android:textSize="30sp"

                    android:layout_marginLeft="50dp"
                    android:id="@+id/p2_manual_hoist_operational_upslowf3_checkbox" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:srcCompat="@drawable/grey_p1board"

                    android:layout_marginLeft="50dp"
                    android:id="@+id/p2_manual_hoist_operational_upf3slow_result_led" />

                   </LinearLayout>
                <CheckBox
                    android:text="Down(SLOW-F4)"
                    android:layout_width="wrap_content"
                    android:textSize="30sp"
                    android:layout_height="wrap_content"

                    android:layout_marginLeft="100dp"
                    android:id="@+id/p2_manual_hoist_operational_downslowf4_checkbox" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:srcCompat="@drawable/grey_p1board"


                    android:layout_marginLeft="51dp"
                    android:id="@+id/p2_manual_hoist_operational_downf4slow_result_led" />


            </LinearLayout>
            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="850dp"
            android:orientation="vertical"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:background="@drawable/output_line_for_indiviudal_tasks"
            android:layout_marginTop="10dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:background="#CEBBC9"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:layout_margin="5dp">


                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Individual Outputs"
                            android:textSize="30sp"
                            android:id="@+id/textView2"
                            android:layout_marginLeft="15dp"
                            android:layout_marginTop="10dp" />

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:orientation="horizontal"
                    android:background="#CEBBC9"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp">
                    <CheckBox
                        android:text="F3 HIGH"
                        android:textSize="30sp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:padding="10dp"
                        android:layout_marginLeft="50dp"
                        android:id="@+id/p2_manual_hoist_individual_F3High_checkbox" />
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:orientation="horizontal"
                        android:layout_height="wrap_content"
                        android:id="@+id/p2_manual_hoist_individual_F3High_result_layout">

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            app:srcCompat="@drawable/grey_p1board"
                            android:padding="10dp"
                            android:id="@+id/p2_manual_hoist_individual_upf3fast_result_led"
                            android:layout_marginLeft="5dp" />

                        <RadioGroup
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/p2_manual_hoist_individual_F3High_radioGroup"
                            android:orientation="horizontal"
                            >



                            <RadioButton
                                android:text="ON State"
                                android:textSize="18sp"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:padding="10dp"
                                android:layout_marginLeft="50dp"
                                android:id="@+id/p2_manual_hoist_individual_F3High_radio_ON"
                                android:layout_weight="1" />

                            <RadioButton
                                android:text="OFF State"
                                android:layout_width="wrap_content"
                                android:textSize="18sp"
                                android:layout_height="wrap_content"
                                android:padding="10dp"
                                android:layout_marginLeft="50dp"
                                android:id="@+id/p2_manual_hoist_individual_F3High_radio_OFF"
                                android:layout_weight="1" />
                        </RadioGroup>

                    </LinearLayout>



                </LinearLayout>

                <LinearLayout
                            android:layout_width="wrap_content"
                            android:background="#CEBBC9"
                            android:orientation="horizontal"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="10dp">
                            <CheckBox
                                android:text="F3 LOW"
                                android:layout_width="wrap_content"
                                android:textSize="30sp"
                                android:layout_height="wrap_content"
                                android:padding="10dp"
                                android:layout_marginLeft="50dp"
                                android:id="@+id/p2_manual_hoist_individual_F3Low_checkbox" />
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:orientation="horizontal"
                                android:layout_height="wrap_content"
                                android:id="@+id/p2_manual_hoist_individual_F3Low_result_layout">

                                <ImageView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    app:srcCompat="@drawable/grey_p1board"
                                    android:padding="10dp"
                                    android:id="@+id/p2_manual_hoist_individual_upf4slow_result_layout"
                                    android:layout_marginLeft="5dp" />

                                <RadioGroup
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:id="@+id/p2_manual_hoist_individual_F3Low_radioGroup"
                                    android:orientation="horizontal"
                                    >

                                    <RadioButton
                                        android:text="ON State"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"

                                        android:textSize="18sp"
                                        android:padding="10dp"
                                        android:layout_marginLeft="50dp"
                                        android:id="@+id/p2_manual_hoist_individual_F3Low_radio_ON" />

                                    <RadioButton
                                        android:text="OFF State"
                                        android:layout_width="wrap_content"
                                        android:textSize="18sp"
                                        android:layout_height="wrap_content"
                                        android:padding="10dp"
                                        android:layout_marginLeft="50dp"
                                        android:id="@+id/p2_manual_hoist_individual_F3Low_radio_OFF" />
                                </RadioGroup>




                            </LinearLayout>



                        </LinearLayout>
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:orientation="horizontal"
                    android:background="#CEBBC9"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_marginBottom="5dp">

                    <CheckBox
                        android:text="F4 HIGH"
                        android:layout_width="wrap_content"
                        android:textSize="30sp"
                        android:layout_height="wrap_content"
                        android:padding="10dp"
                        android:layout_marginLeft="50dp"
                        android:id="@+id/p2_manual_hoist_individual_F4High_checkbox"                    />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:orientation="horizontal"
                        android:layout_height="wrap_content"
                        android:id="@+id/p2_manual_hoist_individual_F4High_result_layout">

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            app:srcCompat="@drawable/grey_p1board"
                            android:padding="10dp"
                            android:layout_marginLeft="10dp"
                            android:id="@+id/p2_manual_hoist_individual_downf3slow_result_led"
                            />

                        <RadioGroup
                            android:layout_width="wrap_content"
                            android:id="@+id/p2_manual_hoist_individual_F4High_radioGroup"
                            android:orientation="horizontal"
                            android:layout_height="wrap_content">
                            <RadioButton
                                android:text="ON State"
                                android:layout_width="wrap_content"
                                android:textSize="18sp"
                                android:layout_height="wrap_content"
                                android:padding="10dp"

                                android:layout_marginLeft="50dp"
                                android:id="@+id/p2_manual_hoist_individual_F4High_radio_ON" />

                            <RadioButton
                                android:text="OFF State"
                                android:layout_width="wrap_content"
                                android:textSize="18sp"
                                android:layout_height="wrap_content"
                                android:padding="10dp"
                                android:layout_marginLeft="50dp"
                                android:id="@+id/p2_manual_hoist_individual_F4High_radio_OFF" />


                        </RadioGroup>



                    </LinearLayout>



                </LinearLayout>
                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:background="#CEBBC9"
                            android:orientation="horizontal"

                            android:layout_height="wrap_content"
                            android:layout_marginLeft="10dp">
                            <CheckBox
                                android:text="F4 LOW"
                                android:layout_width="wrap_content"
                                android:textSize="30sp"
                                android:layout_height="wrap_content"
                                android:padding="10dp"
                                android:layout_marginLeft="50dp"
                                android:id="@+id/p2_manual_hoist_individual_F4Low_checkbox" />
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:orientation="horizontal"

                                android:layout_height="wrap_content"
                                android:id="@+id/p2_manual_hoist_individual_F4Low_result_layout">

                                <ImageView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    app:srcCompat="@drawable/grey_p1board"
                                    android:padding="10dp"
                                    android:layout_marginLeft="10dp"
                                    android:id="@+id/p2_manual_hoist_individual_downf4fast_result_led" />

                                <RadioGroup
                                    android:layout_width="wrap_content"
                                    android:id="@+id/p2_manual_hoist_individual_F4Low_radioGroup"
                                    android:layout_height="wrap_content"
                                    android:orientation="horizontal"
                                    >


                                    <RadioButton
                                        android:text="ON State"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"

                                        android:textSize="18sp"
                                        android:padding="10dp"
                                        android:layout_marginLeft="50dp"
                                        android:id="@+id/p2_manual_hoist_individual_F4Low_radio_ON" />

                                    <RadioButton
                                        android:text="OFF State"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:textSize="18sp"
                                        android:padding="10dp"
                                        android:layout_marginLeft="50dp"
                                        android:id="@+id/p2_manual_hoist_individual_F4Low_radio_OFF" />

                                </RadioGroup>


                            </LinearLayout>



                        </LinearLayout>

            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/p2_manual_hoist_testsBTNs"
            android:layout_width="match_parent"
            android:orientation="horizontal"
            android:layout_height="match_parent"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="8dp">

            <ImageButton
                android:layout_height="65dp"
                android:id="@+id/P2manualHoistTestbtn_id"

                android:background="@drawable/test_btn_both"
                android:layout_width="180dp"
                android:layout_marginLeft="120dp"
                android:layout_marginTop="15dp" />

            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/P2manualHoistDebugbtn_id"
                android:background="@drawable/debug_btn_both"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="15dp" />
            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/P2manualHoistResetbtn_id"
                android:background="@drawable/reset_btn_both"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="15dp" />
        </LinearLayout>

    </LinearLayout>


</LinearLayout>