                android:background="@drawable/rammer_background"
                android:layout_height="wrap_content"
                android:layout_marginTop="15dp"
                android:layout_marginLeft="10dp">
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/rammer_forward"
                    android:orientation="vertical"
                    android:layout_marginTop="60dp"
                    android:layout_marginLeft="20dp"
                    android:layout_marginBottom="20dp">
                    <RelativeLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@drawable/rammer_forward_fast_f33"
                        android:layout_marginTop="60dp"
                        android:layout_marginLeft="40dp">


                        <CheckBox
                            android:text="ON State"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentTop="true"
                            android:layout_alignParentStart="true"
                            android:layout_marginTop="43dp"
                            android:id="@+id/p1_checkbox_c13_On" />

                        <CheckBox
                            android:text="OFF State"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:layout_alignParentStart="true"
                            android:id="@+id/p1_checkbox_c13_Off" />

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            app:srcCompat="@drawable/new_green_led"
                            android:layout_alignTop="@+id/p1_checkbox_c13_On"
                            android:layout_toEndOf="@+id/p1_checkbox_c13_Off"
                            android:layout_marginStart="79dp"
                            android:id="@+id/p1_c13_led_result" />

                        <TextView
                            android:text="TextView"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignTop="@+id/p1_c13_led_result"
                            android:layout_toEndOf="@+id/p1_c13_led_result"
                            android:id="@+id/p1_c13_textview_result"
                            android:layout_marginTop="7dp" />

                        <ImageButton
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/p1_debug_btn_c13"
                            android:layout_below="@+id/p1_checkbox_c13_Off"
                            android:layout_alignParentStart="true"
                            android:layout_marginTop="100dp"
                            android:background="@drawable/debugbutton"
                            android:layout_marginLeft="10dp" />

                        <ImageButton
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/p1_check_btn_c13"
                            android:background="@drawable/checkbutton"
                            android:layout_below="@+id/p1_c13_led_result"
                            android:layout_alignStart="@+id/p1_c13_led_result"
                            android:layout_marginTop="8dp" />

                        <ImageButton
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/p1_reset_btn_c13"
                            android:background="@drawable/resetbutton"
                            android:layout_alignTop="@+id/p1_debug_btn_c13"
                            android:layout_alignStart="@+id/p1_check_btn_c13" />


