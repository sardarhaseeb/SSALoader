                            android:id="@+id/p1_check_btn_c14"
                            android:background="@drawable/checkbutton"
                            android:layout_below="@+id/p1_c14_led_result"
                            android:layout_alignStart="@+id/p1_c14_led_result"
                            android:layout_marginTop="8dp" />

                        <ImageButton
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/p1_reset_btn_c14"
                            android:background="@drawable/resetbutton"
                            android:layout_alignTop="@+id/p1_debug_btn_c14"
                            android:layout_alignStart="@+id/p1_check_btn_c14" />







                    </RelativeLayout>
                    <RelativeLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@drawable/rammer_forward_f33f34"
                        android:layout_marginLeft="40dp"
                        android:layout_marginTop="20dp"
                        android:layout_marginBottom="20dp">


                        />
                        <CheckBox
                            android:text="ON State"
                            android:layout_width="wrap_content"
