                android:layout_marginTop="60dp">


                <CheckBox
                    android:text="ON State"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentTop="true"
                    android:layout_alignParentStart="true"
                    android:layout_marginTop="43dp"
                    android:id="@+id/p1_checkbox_c13_On" />

                <CheckBox
                    android:text="OFF State"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_alignParentStart="true"
                    android:id="@+id/p1_checkbox_c13_Off" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:srcCompat="@drawable/new_green_led"
                    android:layout_alignTop="@+id/p1_checkbox_c13_On"
                    android:layout_toEndOf="@+id/p1_checkbox_c13_Off"
                    android:layout_marginStart="79dp"
                    android:id="@+id/p1_c13_led_result" />

                <TextView
                    android:text="TextView"
